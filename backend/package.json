{
  "name": "kadongsite-backend",
  "version": "1.0.0",
  "description": "Backend API for KaDong Tools",
  "main": "app.js",
  "type": "module",
  "scripts": {
    "start": "node app.js",
    "dev": "nodemon app.js",
    "db:migrate:up": "node scripts/migrate.js up",
    "db:migrate:down": "node scripts/migrate.js down",
    "db:migrate:status": "node scripts/migrate.js status",
    "db:seed": "node scripts/seed.js",
    "db:test": "node scripts/test-db.js",
    "db:setup": "npm run db:migrate:up && npm run db:seed",
    "gold:fetch": "node scripts/fetch-gold.js",
    "gold:fetch:cron": "node scripts/fetch-gold.js --cron",
    "gold:migrate": "node scripts/migrate-gold-localstorage.js"
  },
  "keywords": [
    "express",
    "api",
    "tools",
    "postgresql"
  ],
  "author": "KaDong",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.13.2",
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.2",
    "cheerio": "^1.1.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^8.2.1",
    "node-cron": "^4.2.1",
    "pg": "^8.11.3",
    "validator": "^13.15.23"
  },
  "devDependencies": {
    "nodemon": "^3.0.2"
  }
}
